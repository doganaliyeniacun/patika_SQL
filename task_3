--1.
SELECT *
FROM 
 COUNTRY 
WHERE COUNTRY ILIKE 'A%a';
--2.
SELECT *
FROM 
 COUNTRY
WHERE 
 LENGTH(COUNTRY) >= 6 AND 
 COUNTRY LIKE '%n';
--3.
SELECT 
 TITLE 
FROM 
 FILM 
WHERE 
 TITLE ILIKE '%T%T%T%T%';
--4.
SELECT 
 TITLE,
 F.LENGTH,
 RENTAL_RATE
FROM 
 FILM AS F
WHERE 
 TITLE ILIKE 'C%' AND
 F.LENGTH > 90 AND
 RENTAL_RATE = 2.99;
 
