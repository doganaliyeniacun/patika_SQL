--1.
SELECT
 RATING
FROM 
 FILM
GROUP BY 
 RATING;
 
--2.
SELECT
 REPLACEMENT_COST,
 COUNT(REPLACEMENT_COST) 
FROM
 FILM
GROUP BY 
 REPLACEMENT_COST
HAVING
 COUNT(REPLACEMENT_COST) > 50;
 
--3.
SELECT
 STORE_ID,
 COUNT(STORE_ID)
FROM
 CUSTOMER
GROUP BY
 STORE_ID;

--4.
SELECT
 COUNTRY_ID,
 COUNT(CITY)
FROM 
 CITY
GROUP BY 
 COUNTRY_ID
ORDER BY COUNT(CITY) DESC
LIMIT 1
 


